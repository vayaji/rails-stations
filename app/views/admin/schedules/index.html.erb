<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>映画名</th>
            <th>開始時間</th>
            <th>終了時間</th>
            <th>作成日時</th>
            <th>更新日時</th>
        </tr>
    </thead>
    <tbody>
        <% @schedules.each do |schedule| %>
            <tr>
                <td><%= link_to schedule.id, admin_schedule_path(schedule) %></td>
                <td><%= schedule.movie.name %></td>
                <td><%= schedule.start_time.strftime("%Y-%m-%d %H:%M") %></td>
                <td><%= schedule.end_time.strftime("%Y-%m-%d %H:%M") %></td>
                <td><%= schedule.created_at.strftime("%Y-%m-%d %H:%M") %></td>
                <td><%= schedule.updated_at.strftime("%Y-%m-%d %H:%M") %></td>
            </tr>
        <% end %>
</table>